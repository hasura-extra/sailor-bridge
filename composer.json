{
    "name": "hasura-extra/sailor-bridge",
    "license": "MIT",
    "description": "Provide integration for Sailor graphql client with Hasura Extra packages.",
    "autoload": {
        "psr-4": {
            "Hasura\\SailorBridge\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hasura\\SailorBridge\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/hasura-sailor"
    ],
    "require": {
        "php": ">=8.0",
        "spawnia/sailor": "^v0.10",
        "hasura-extra/api-client": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/filesystem": "^5.0"
    },
    "conflict": {
        "hasura-extra/auth-hook": "<2.1.2",
        "hasura-extra/event-dispatcher": "<2.1.2",
        "hasura-extra/bundle": "<2.1.2",
        "hasura-extra/graphqlite-bridge": "<2.1.2",
        "hasura-extra/metadata": "<2.1.2",
        "hasura-extra/laravel": "<2.1.2"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
